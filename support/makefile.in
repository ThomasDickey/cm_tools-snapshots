# $Id: makefile.in,v 11.1 1997/09/13 16:37:33 tom Exp $
# support scripts & utilities



@make_include_left@@TD_LIB_rules@/td_lib.mk@make_include_right@

####### (Standard Lists) #######################################################
SCRIPTS	= \
	testpath.sh\
	testinit.sh\
	who_suid.sh

C_SRC	= \
	run_tool.c \
	set_date.c

SOURCES	= $(SCRIPTS)

ALL	= \
	run_tool \
	set_date

####### (Standard Productions) #################################################
all:	$(SOURCES) $(ALL)
clean:			; -$(RM) $(CLEAN)
distclean:	clean	; -$(RM) $(ALL)
run_test:
sources:	$(SOURCES)
install:
deinstall:

####### (Details of Productions) ###############################################
Z	= $(CC) $(CFLAGS) $(CPP_OPTS) -o $@ $@.c $(LIB_ARGS)
run_tool:	$(RCSDEFS_H) $(LIB_DEPS) ; $Z
set_date:	$(PTYPES_H)  $(LIB_DEPS) ; $Z
