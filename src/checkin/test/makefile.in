# make-file for testing RCS checkin utility

THIS	= checkin

@make_include_left@@TD_LIB_rules@/td_lib.mk@make_include_right@

####### (Standard Lists) #######################################################
REF_FILES=\
	case1_a.ref\
	case1_b.ref\
	case2_a.ref\
	case2_b.ref\
	case2_c.ref\
	case2_d.ref\
	case3_a.ref\
	case3_b.ref\
	case3_c.ref\
	case3_d.ref\
	case3_e.ref\
	case3_f.ref

SCRIPTS	=\
	admin.sh\
	case1.sh\
	case1_a.sh\
	case1_b.sh\
	case2.sh\
	case2_a.sh\
	case2_b.sh\
	case2_c.sh\
	case2_d.sh\
	case3.sh\
	case3_a.sh\
	case3_b.sh\
	case3_c.sh\
	case3_d.sh\
	case3_e.sh\
	case3_f.sh\
	clean.sh\
	run_test.sh

SOURCES	= makefile.in $(SCRIPTS) $(REF_FILES) README
ALL	=

####### (Standard Productions) #################################################
all:		$(SOURCES) $(ALL)
clean:				; -$(RM) -r $(CLEAN) junk
distclean:	clean		; -$(RM) $(ALL)
lint.out:
check:		$(SOURCES) $(ALL) ; $(RUN_TESTS)
