# $Id: makefile.in,v 11.2 1997/09/28 20:14:16 tom Exp $
# make-file for testing RCS checkout utility

THIS	= checkout

@make_include_left@@TD_LIB_rules@/td_lib.mk@make_include_right@

####### (Standard Lists) #######################################################
SCRIPTS	=\
	run_test.sh

SOURCES	= makefile.in $(SCRIPTS)
ALL	=

####### (Standard Productions) #################################################
all:		$(SOURCES) $(ALL)
clean:				; -$(RM) $(CLEAN)
distclean:	clean		; -$(RM) $(ALL)
lint.out:
run_test:	$(SOURCES) $(ALL) ; $(RUN_TESTS)
