# make-file for testing RCS/sccs checkup utility

THIS	= checkup

@make_include_left@@TD_LIB_rules@/td_lib.mk@make_include_right@

####### (Standard Lists) #######################################################
SCRIPTS	=\
	run_test.sh

SOURCES	= makefile.in $(SCRIPTS)

####### (Standard Productions) #################################################
all:		$(SOURCES)
