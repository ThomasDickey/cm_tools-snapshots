# $Id: makefile.in,v 11.7 2020/12/08 20:39:43 tom Exp $
# MAKE-file for RCS skeleton tree utility

####### (Environment) ##########################################################
SHELL		= /bin/sh
top_srcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@
@SET_MAKE@

THIS	= link2rcs

@make_include_left@@TD_LIB_rules@/td_lib.mk@make_include_right@

BUILD_INC	= $(TD_INC)
BUILD_LIB	= $(TD_LIB)

CC		= @CC@
CFLAGS		= @CFLAGS@
EXTRA_CFLAGS	= @EXTRA_CFLAGS@

####### (Standard Lists) #######################################################
B	= ../bin
ALL	= $B/$(THIS)
CSRC	= $(THIS).c
SOURCES	= $(CSRC)

####### (Standard Productions) #################################################
all\
install:	$(SOURCES) $B $(ALL)

clean:				; -$(RM) $(CLEAN)
distclean: 	clean		; -$(RM) $(ALL)
check:

####### (Details of Productions) ###############################################
$B:				; mkdir $@
$B/$(THIS):	$(THIS).o $(LIB_DEPS)	; $(LINK) -o $@ $(THIS).o $(LIB_ARGS)
$(THIS).o:	$(RCSDEFS_H)

####### (Development) ##########################################################
lint.out:	$(CSRC)		; $(DATE); tdlint $(LINTOPTS) $? >>$@
