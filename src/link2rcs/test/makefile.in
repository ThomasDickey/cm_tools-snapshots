# make-file for testing RCS skeleton tree utility

THIS	= link2rcs

@make_include_left@@TD_LIB_rules@/td_lib.mk@make_include_right@

####### (Standard Lists) #######################################################
REF_FILES =\
	case1.ref\
	case2.ref

SCRIPTS	=\
	listtree.sh\
	run_test.sh

SOURCES	= makefile.in $(SCRIPTS) $(REF_FILES)

####### (Standard Productions) #################################################
all:		$(SOURCES)
clean:				; -$(RM) $(CLEAN)
distclean:	clean
lint.out:
run_test:	$(SOURCES)	; $(RUN_TESTS)
